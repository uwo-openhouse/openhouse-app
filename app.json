{
  "expo": {
    "name": "UWO Open House App",
    "slug": "openhouse_app",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.uwo.openhouse",
      "permissions": [
        "ACCESS_FINE_LOCATION"
      ],
      "versionCode": 1,
      "icon": "./src/assets/images/icon.png"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "The app uses the location to place a marker representing the user on maps."
      }
    }
  }
}
